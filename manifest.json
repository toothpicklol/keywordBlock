{
    "name": "keywordBlocker",
    "description": "keywordBlockerOnYoutube",
    "version": "1.0",
    "manifest_version": 3,
    "action":
    {
        "default_popup": "main.html",
        "default_icon": "block_icom.png"
    },
    "permissions": [
        "storage", "activeTab", "scripting"
    ],
    "content_scripts": [
    {
        "matches": ["*://*.youtube.com/*"],
        "js": ["blocker.js"]

    }]
}